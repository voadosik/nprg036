<?xml version="1.0" encoding="UTF-8"?>
<festivalSchedule xmlns:xsi="http://www.w3.org/2001/XMLSchema"
                  xsi:noNamespaceSchemaLocation="../schemas/schema-1.xsd">

    <movies>
        <movie iri="http://example.org/data/drive">
            <name xml:lang="en">Drive</name>
            <label xml:lang="en">Drive</label>
            <durationMinutes>100</durationMinutes>
            <datePublished>2011</datePublished>
            <awardRef iri="http://example.org/data/best-director-award"/>
            <screeningRef iri="http://example.org/data/screening-hydropark"/>
        </movie>
        <movie iri="http://example.org/data/blade-runner-2049">
            <name xml:lang="en">Blade Runner 2049</name>
            <label xml:lang="en">Blade Runner 2049</label>
            <durationMinutes>164</durationMinutes>
            <datePublished>2017</datePublished>
            <awardRef iri="http://example.org/data/oscar-cinematography"/>
            <screeningRef iri="http://example.org/data/screening-slovansky-dum"/>
        </movie>
        <movie iri="http://example.org/data/la-la-land">
            <name xml:lang="en">La La Land</name>
            <label xml:lang="en">La La Land</label>
            <durationMinutes>128</durationMinutes>
            <datePublished>2016</datePublished>
            <awardRef iri="http://example.org/data/oscar-actress"/>
            <screeningRef iri="http://example.org/data/screening-guliver"/>
        </movie>
    </movies>

    <screenings>
        <screening iri="http://example.org/data/screening-hydropark">
            <name xml:lang="en">Hydropark Screening</name>
            <label xml:lang="en">Drive screening at Hydropark</label>
            <startDate>2011-11-11T16:30:00</startDate>
            <movieRef iri="http://example.org/data/drive"/>
            <locationRef iri="http://example.org/data/location-hydropark"/>
        </screening>
        <screening iri="http://example.org/data/screening-slovansky-dum">
            <name xml:lang="en">Slovansky dum Screening</name>
            <label xml:lang="en">Blade Runner 2049 screening at Slovansky dum</label>
            <startDate>2017-07-16T21:00:00</startDate>
            <movieRef iri="http://example.org/data/blade-runner-2049"/>
            <locationRef iri="http://example.org/data/location-slovansky-dum"/>
        </screening>
        <screening iri="http://example.org/data/screening-guliver">
            <name xml:lang="en">Guliver Screening</name>
            <label xml:lang="en">La La Land screening at Guliver</label>
            <startDate>2016-05-27T12:15:00</startDate>
            <movieRef iri="http://example.org/data/la-la-land"/>
            <locationRef iri="http://example.org/data/location-guliver"/>
        </screening>
    </screenings>

    <places>
        <place iri="http://example.org/data/location-hydropark">
            <label xml:lang="en">Hydropark</label>
        </place>
        <place iri="http://example.org/data/location-slovansky-dum">
            <label xml:lang="en">Slovansky dum</label>
        </place>
        <place iri="http://example.org/data/location-guliver">
            <label xml:lang="en">Guliver</label>
        </place>
    </places>

    <awards>
        <award iri="http://example.org/data/best-director-award">
            <name xml:lang="en">Best Director Award</name>
            <label xml:lang="en">Best Director Award</label>
            <prizeMoney>10000</prizeMoney>
            <movieRef iri="http://example.org/data/drive"/>
        </award>
        <award iri="http://example.org/data/oscar-cinematography">
            <name xml:lang="en">Oscar Best Cinematography</name>
            <label xml:lang="en">Oscar Best Cinematography</label>
            <prizeMoney>15000</prizeMoney>
            <movieRef iri="http://example.org/data/blade-runner-2049"/>
        </award>
        <award iri="http://example.org/data/oscar-actress">
            <name xml:lang="en">Oscar Best Actress</name>
            <label xml:lang="en">Oscar Best Actress</label>
            <prizeMoney>20000</prizeMoney>
            <movieRef iri="http://example.org/data/la-la-land"/>
        </award>
    </awards>

</festivalSchedule>